---
// @ts-nocheck
import LegacyLayout from "../../layouts/LegacyLayout.astro";
import LegacyHeader from "../../components/LegacyHeader.astro";
import LegacyFooter from "../../components/LegacyFooter.astro";
---
<LegacyLayout
  title="Exposure - MP.LS"
  description="Kid Kaija's guide to winter exposure and staying safe in cold weather."
>
<div class="mpls-legacy">
<LegacyHeader showNav />

<p><!-- Header Bar --></p>
<div class="cool-header">
<div class="cool-bar"></div>
</div>
<p><!-- Gallery Grid --></p>
<div class="exposure-grid">
<div class="row"><a href="/"><img decoding="async" src="/img/01.jpg" alt="Exposure 01"></a><br>
<a href="/"><img decoding="async" src="/img/02.jpg" alt="Exposure 02"></a><br>
<a href="/"><img decoding="async" src="/img/03.jpg" alt="Exposure 03"></a><br>
<a href="/"><img decoding="async" src="/img/04.jpg" alt="Exposure 04"></a></div>
<div class="row"><a href="/"><img decoding="async" src="/img/12.jpg" alt="Exposure 12"></a><br>
<img decoding="async" class="mid-banner" src="/img/ne_words.gif" alt="Northern Exposure Text"><br>
<a href="/"><img decoding="async" src="/img/05.jpg" alt="Exposure 05"></a></div>
<div class="row info-row"><a href="/"><img decoding="async" src="/img/11.jpg" alt="Exposure 11"></a>
<div class="info-box">
<p>Click on an image for a larger view and description.</p>
<p>Visit <a href="https://heinrich.mpls.com/" target="_blank" rel="noopener">Photographer George Heinrich's website</a> for more dramatic photography and studio portfolio.</p>
<p><img decoding="async" src="/img/mpls.gif" alt="mpls.com logo" width="95" height="18"></p>
</div>
<p><a href="/"><img decoding="async" src="/img/06.jpg" alt="Exposure 06"></a></p>
</div>
<div class="row"><a href="/"><img decoding="async" src="/img/10.jpg" alt="Exposure 10"></a><br>
<a href="/"><img decoding="async" src="/img/09.jpg" alt="Exposure 09"></a><br>
<a href="/"><img decoding="async" src="/img/08.jpg" alt="Exposure 08"></a><br>
<a href="/"><img decoding="async" src="/img/07.jpg" alt="Exposure 07"></a></div>
</div>
<LegacyFooter />
</div>
</LegacyLayout>












