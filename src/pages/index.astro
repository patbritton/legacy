<script>
  const banner = document.getElementById("legacyBanner");
  const spacer = document.getElementById("legacyBannerSpacer");
  const closeButton = document.getElementById("legacyBannerClose");
  if (banner && closeButton) {
    document.body.classList.add("has-legacy-banner");
    requestAnimationFrame(() => banner.classList.add("show"));
    closeButton.addEventListener("click", () => {
      banner.classList.remove("show");
      banner.style.display = "none";
      if (spacer) {
        spacer.style.display = "none";
      }
      document.body.classList.remove("has-legacy-banner");
    });
  }
</script>






